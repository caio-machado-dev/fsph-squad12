{
  "expo": {
    "name": "fsph-squad12-app",
    "slug": "fsph-squad12-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "fsphsquad12app",
    "platforms": ["ios", "android", "web"],
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "extra": {
      "GOOGLE_WEB_CLIENT_ID": "SEU_CLIENT_ID_WEB_AQUI",
      "GOOGLE_ANDROID_CLIENT_ID": "SEU_CLIENT_ID_ANDROID_AQUI",
      "GOOGLE_IOS_CLIENT_ID": "SEU_CLIENT_ID_IOS_AQUI"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.fsphsquad12app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
