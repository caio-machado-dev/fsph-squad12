{
  "name": "fsph-squad12",
  "version": "1.0.0",
  "description": "FSPH Squad 12 - Monorepo with React Native mobile app and Express backend",
  "private": true,
  "workspaces": [
    "mobile",
    "backend",
    "shared"
  ],
  "scripts": {
    "mobile": "cd mobile && npm run start",
    "mobile:dev": "cd mobile && npm run start",
    "mobile:build": "cd mobile && npm run build",
    "backend": "cd backend && npm run dev",
    "backend:dev": "cd backend && npm run dev",
    "backend:start": "cd backend && npm run start",
    "shared:build": "cd shared && npm run build",
    "install:all": "npm install && npm run install:mobile && npm run install:backend && npm run install:shared",
    "install:mobile": "cd mobile && npm install",
    "install:backend": "cd backend && npm install",
    "install:shared": "cd shared && npm install",
    "lint": "npm run lint:mobile && npm run lint:backend && npm run lint:shared",
    "lint:mobile": "cd mobile && npm run lint",
    "lint:backend": "cd backend && npm run lint",
    "lint:shared": "cd shared && npm run lint",
    "test": "npm run test:backend",
    "test:backend": "cd backend && npm run test",
    "dev": "concurrently \"npm run backend:dev\" \"npm run mobile:dev\"",
    "clean": "rm -rf node_modules mobile/node_modules backend/node_modules shared/node_modules"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/fsph-squad12.git"
  },
  "keywords": [
    "react-native",
    "express",
    "nodejs",
    "monorepo",
    "mobile",
    "api"
  ],
  "author": "FSPH Squad 12",
  "license": "MIT"
}